<% if not admin_status %>
	<%= controller.redirect_to '/' %>
<% else %>

	<% @title="Manage Problems" %>
	<h1 class="text-center p-1">Manage Problems</h1>
	<table class="table table-striped table-hover">
		<thead>
			<tr class="text-center">
				<th scope="col" class="text-left">Problem</th>
				<th scope="col">Average Score</th>
				<th scope="col">Points</th>
				<th scope="col">Hidden</th>
			</tr>
		</thead>

		<tbody class="table_here">

		</tbody>
	</table>

	<%= link_to "Update Table", "/update_problem_table", remote: true, method: :patch, :id => "click_me", :class => "d-none" %>
<script type="text/javascript">
document.getElementById("click_me").click()
</script>

<% end %>